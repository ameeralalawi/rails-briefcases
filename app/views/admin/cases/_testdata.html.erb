<div class="around">
  <div class="Section-Header">
    <h3 class= page-title> PREVIEW AND FILL TEST DATA</h3>
    <div class="save-button">
      <%= button_tag type: 'submit', form: "save-testdata-form", class: "fabutton save-btn-inner", id: "global-save-4" do %>
      <i class="fa fa-2x fa-floppy-o" aria-hidden="true"></i>
      <h4> CASE</h4><% end %>
    </div>
  </div>
  <div id= "testdata_first">
    <div id="preview">
      <%= simple_form_for @case, url: save_testdata_admin_case_path(@case), :html => {:id => "save-testdata-form"} ,method: :patch do |f|%>
        <div id="input-form" class="text-input">
        </div>
      <% end %>
    </div>
  </div>
  <div class="" id="testdata_second">
    <div class="arrow-group">
      <div class="arrow-text">
        <h3>SCENARIO BUILDER</h3>
      </div>
      <div class="arrow bounce" id="to-fifthPage">
        <a class="fa fa-arrow-down fa-2x b-arrow" href="#fifthPage"></a>
      </div>
    </div>
  </div>
</div>
